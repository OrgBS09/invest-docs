[Unit]
Description=Planfix Webhook (Invest, ros)
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/invest-services
EnvironmentFile=/home/ubuntu/invest-services/env/.env.invest
ExecStart=/usr/bin/python3 /home/ubuntu/invest-services/apps/pf_invest_webhook/app.py
Restart=always

[Install]
WantedBy=multi-user.target
